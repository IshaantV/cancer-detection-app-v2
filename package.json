    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "web3": "^4.2.0",
    "ipfs-http-client": "^60.0.1",
    "openai": "^4.20.0",
    "uuid": "^9.0.1",
    "cloudinary": "^1.41.0",
    "google-auth-library": "^9.0.0",
    "pdfkit": "^0.14.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
